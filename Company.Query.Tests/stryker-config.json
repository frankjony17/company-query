{
  "stryker-config": {
    "test-runner": "vstest",
    "reporters": [ "progress", "html" ],
    "log-level": "info",
    "timeout-ms": 15000,
    "project-file": "Company.Query.Domain.csproj",
    "max-concurrent-test-runners": 4,
    "threshold-high": 90,
    "threshold-low": 70,
    "threshold-break": 0,
    "mutate": [ "!**/*Acl.cs" ],
    "files-to-exclude": [],
    "excluded-mutations": [ "string" ],
    "ignore-methods": [ "ToString", "LogInformation", "LogError", "Append", "FirstOrDefault", "First", "GetAllowRefund" ]
  }
}